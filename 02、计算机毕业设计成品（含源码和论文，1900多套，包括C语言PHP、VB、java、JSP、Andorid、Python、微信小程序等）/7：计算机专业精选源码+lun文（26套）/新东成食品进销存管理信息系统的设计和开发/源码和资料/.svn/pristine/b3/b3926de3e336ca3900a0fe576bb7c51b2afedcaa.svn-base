<table class="t1">
<tr>
	<td>分类名</td>
	<td><input type="text" name="className" required value="${foodClass.className }"/></td>
</tr>
<tr>
	<td></td>
	<td><input type="submit" value="保存">  </td>
</tr>
</table>
</form>
