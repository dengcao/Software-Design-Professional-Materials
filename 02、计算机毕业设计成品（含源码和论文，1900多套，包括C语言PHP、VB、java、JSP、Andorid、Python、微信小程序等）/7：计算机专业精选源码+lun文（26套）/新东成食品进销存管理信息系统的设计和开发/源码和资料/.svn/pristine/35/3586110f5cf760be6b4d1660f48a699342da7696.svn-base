<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@include file="/common/public.jsp" %>    
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
	<script type="text/javascript" src="${ctx}/js/checkFrom.js"></script>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>编辑供应商分类</title>
</head>
<body>
<a href='${ctx }/provider/list' >返回供应商列表</a>

<hr />
<form action="${ctx }/provider/update" method="post">
<input type="hidden" name="providerId" value="${provider.providerId}">
<table class="t1">
<tr>
	<td>供应商名称</td>
	<td><input type="text" name="providerName" required value="${provider.providerName }"/></td>
</tr>
	<tr>
		<td>联系人</td>
		<td><input type="text" name="linkman" required value="${provider.linkman }"/></td>
	</tr>
	<tr>
		<td>联系人号码</td>
		<td><input type="text" name="linkNumber" required  value="${provider.linkNumber }"/></td>
	</tr>
	<tr>
		<td>联系人地址</td>
		<td><input type="text" name="providerAdress" required value="${provider.providerAdress }"/></td>
	</tr>
	<tr>
		<td>备注</td>
		<td><input type="text" name="remarks" value="${provider.remarks }"/></td>
	</tr>
<tr>
	<td></td>
	<td><input type="submit" value="保存">  </td>
</tr>
</table>
</form>
</body>
</html>