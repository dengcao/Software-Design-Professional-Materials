<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@include file="/common/public.jsp" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>新增用户</title>
	<script type="text/javascript" src="${ctx}/js/checkFrom.js"></script>
</head>
<body>
<input type="hidden" id="calendar" />
<a href="${ctx}/user/list">返回列表</a>
<hr />
<form action="${ctx }/user/save" method="post">
<table>
<tr>
	<td>用户名</td>
	<td><input type="text" name="username" required/></td>
</tr>
	<tr>
		<td>密码</td>
		<td><input type="text" name="userPSW" required/></td>
	</tr>
<tr>
	<td>生日</td>
	<td><input type="text" name="birthday" readonly id="date-input" /></td>
</tr>	
<tr>
	<td>性别</td>
	<td>
		男<input type="radio" name="sex" value="1" checked />
		女<input type="radio" name="sex" value="2" />
	</td>
</tr>	
<tr>
	<td>地址</td>
	<td><input type="text" name="address" required /></td>
</tr>
	<tr>

		<td><input type="hidden" name="userPower" value="4" /></td>
	</tr>
<tr>
	<td></td>
	<td><input type="submit" value="保存"> </td>
</tr>
</table>
</form>
</body>
</html>